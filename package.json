{
	"name": "monirail",
	"version": "0.1.0",
	"description": "Monitor Railway services and notify your team when anomalies are detected",
	"scripts": {
		"prepublishOnly": "mise run 'check:*' && mise run build",
		"test": "lol nah"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredLunde/monirail.git"
	},
	"keywords": ["railway", "observability", "monitor", "notifier"],
	"author": "Jared Lunde",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jaredLunde/monirail/issues"
	},
	"homepage": "https://github.com/jaredLunde/monirail#readme",
	"sideEffects": false,
	"module": "dist/index.mjs",
	"types": "dist/index.d.mts",
	"exports": {
		"./*": {
			"import": {
				"types": "./dist/*.d.mts",
				"default": "./dist/*.mjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": ["/dist"],
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.5",
		"@graphql-codegen/typescript": "^4.1.4",
		"@graphql-codegen/typescript-graphql-request": "^6.2.0",
		"@graphql-codegen/typescript-operations": "^4.5.0",
		"@types/node": "^20.17.19",
		"bun-types": "^1.2.3",
		"graphql": "^16.10.0",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"graphql-request": "^6.1.0"
	}
}
